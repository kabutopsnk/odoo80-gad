<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
	<!--REGISTRAR AVANCE-->
	<record id="view_wizard_project_task_avance" model="ir.ui.view">
            <field name="name">Wizard Registra Avance Actividad</field>
            <field name="model">wizard.project_task_avance</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Registrar Avance">
		   <group colspan="4" col="8">
			<field name="task_id" colspan="6" readonly="1"/>
			<group colspan="2"/>
			<field name="fecha" colspan="2" required="1"/>
			<group colspan="6"/>
			<field name="name" colspan="6" required="1"/>
			<group colspan="2"/>
			<field name="porcentaje" colspan="2" required="1"/>
			<group colspan="6"/>
			<field name="documento" colspan="8" />
			<newline/>
	                <group colspan="4">
        	                <button icon="gtk-apply" name="registra_avance" confirm="Seguro desea Registrar el Avance %?"  string="Registrar Avance" type="object" default_focus="1" colspan="2"/>
				<group colspan="2"/>
	                </group>
                    </group>
                </form>
            </field>
        </record>
        <record id="action_wizard_project_task_avance" model="ir.actions.act_window">
            <field name="name">Registrar Avance de Actividad</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">wizard.project_task_avance</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>	



    <!--ACTUALIZA ADMINISTRADOR-->
	<record id="view_wizard_contrato_actualiza_administrador" model="ir.ui.view">
            <field name="name">Wizard Actualiza Administrador de Contrato</field>
            <field name="model">wizard.contrato.actualiza_administrador</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Actualiza Administrador de Contrato">
		           <group colspan="4" col="8">
			        <field name="contrato_id" colspan="6" readonly="1"/>
			        <group colspan="2"/>
			        <field name="administrador_id" colspan="4" required="1"/>
			        <group colspan="4"/>
			        <field name="fecha" colspan="2" required="1"/>
			        <group colspan="6"/>
			        <field name="observaciones" colspan="8" />
			        <newline/>
	                <group colspan="4">
                	    <button icon="gtk-apply" name="actualiza_administrador" confirm="Seguro desea Actualizar el Administrador del Contrato?"  string="Actualiza Administrador" type="object" default_focus="1" colspan="2"/>
				        <group colspan="2"/>
	                </group>
                    </group>
                </form>
            </field>
        </record>
        <record id="action_wizard_actualiza_administrador" model="ir.actions.act_window">
            <field name="name">Actualiza Administrador de Contrato</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">wizard.contrato.actualiza_administrador</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>	



